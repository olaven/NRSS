{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts"
  },
  "importMap": "./import_map.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "fmt": {
    "exclude": [
      "output"
    ],
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 4,
    "proseWrap": "preserve"
  }
}